/* MadCap Table Style: */

mcTableStyle
{
	name: TipTable;
	border-collapse: collapse;
	border-left-style: solid;
	border-left-width: 2px;
	border-right-style: solid;
	border-right-width: 2px;
	border-top-style: solid;
	border-top-width: 2px;
	border-bottom-style: solid;
	border-bottom-width: 2px;
	cell-padding-left: 2px;
	cell-padding-right: 2px;
	cell-padding-top: 2px;
	cell-padding-bottom: 2px;
	border-spacing-y: 2;
	border-spacing-x: 2;
	border-left-color: #ffffd1;
	border-right-color: #ffffd1;
	border-top-color: #ffffd1;
	border-bottom-color: #ffffd1;
	margin-top: 12px;
	margin-bottom: 12px;
}

mcTableColumnStyle
{
	span: 1;
	separator-style: solid;
	separator-width: 0px;
	separator-color: #000000;
	background-color: #ffffd1;
	font-size: 8pt;
	font-family: Tahoma;
	color: #008000;
	font-weight: normal;
	font-style: normal;
}

mcTableBodyStyle
{
	span: 1;
	font-weight: normal;
	separator-style: solid;
	separator-width: 0px;
	separator-color: #000000;
	background-color: #ffffd1;
	font-size: 8pt;
	font-family: Tahoma;
	color: #008000;
	font-style: normal;
}

/* Cell Styles: */

