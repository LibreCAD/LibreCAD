	border-top-width: 2px;
	border-bottom-style: solid;
	border-bottom-color: #ffffd1;
	border-bottom-width: 2px;
	border-collapse: collapse;
	border-spacing: 2px 2px;
	mc-cell-padding: 2px 2px;
